<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:m='urn:import:gwt.material.design.client.ui'
             xmlns:ma='urn:import:gwt.material.design.addins.client'>

    <ui:with field="bundle" type="com.progressoft.security.userinfo.client.views.UserInfoBundle"/>
    <ui:style gss="true">
        .sample-div{
        background-color: #CECECE;
        }
    </ui:style>

    <ma:sideprofile.MaterialSideProfile ui:field="root" url="http://static1.squarespace.com/static/51609147e4b0715db61d32b6/521b97cee4b05f031fd12dde/5519e33de4b06a1002802805/1431718693701/?format=1500w">
        <ma:avatar.MaterialAvatar ui:field="userAvatar" name="" width="100" height="100" />
        <m:MaterialChip ui:field="userChip" addStyleNames="{bundle.style.userDisplayName}" letterColor="WHITE" letterBackgroundColor="BLUE_DARKEN_3" text="" backgroundColor="BLUE_LIGHTEN_2" textColor="WHITE"></m:MaterialChip>
    </ma:sideprofile.MaterialSideProfile>



</ui:UiBinder>